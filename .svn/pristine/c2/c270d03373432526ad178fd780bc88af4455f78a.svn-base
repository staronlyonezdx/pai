.imgs img:nth-child(2){
    width: 3.06rem;
    height: 0.22rem;
    margin-top: 0.19rem;
}


.call_no{
    width: 5.8rem;
    height: 6.47rem;
    position: relative;
    display: none;
    /*background:#fff;*/
    background: url("../../image/new_people/pic_tanchuang1@2x.png") center center;
    background-size: 100%;
    border-radius: 0.18rem;
   /*padding-bottom: 0.44rem;*/
}
.call_no .value_under{
    width:5.8rem;
    height:1.98rem;
    color: #FFFFFF;
    background: url("../../image/new_people/pic_beijing@2x.png") no-repeat;
}

.call_no .value_under .value_under_top{
    text-align: center;
    padding-top: 0.33rem;
}
.call_no .value_under .value_under_top span{
    font-size: 0.3rem;
    font-weight:bold;
}
.call_no .value_under .value_under_top img{
    width: 0.56rem;
    height: 0.56rem;
    transform: translateX(0.6rem);
}
.call_no .value_under .value_under_bottom{
    margin-left: 0.75rem;
    font-size: 0.24rem;
    line-height: 0.5rem;
}
.call_no .tips{
    text-align: center;
}
.call_no .tips img{
    width: 2.23rem;
    height: 0.44rem;
    margin-top: 0.2rem;
}
.call_no .tips .tips_spans{
    margin-top: 0.2rem;
    color: #AAAAAA ;
    font-size: 0.24rem;
    text-align: left;
    line-height: 0.48rem;
    padding: 0 0.3rem;
}
.call_no .tips .tips_spans span{
    margin-left: 0.1rem;
}
.call_no .buttons a,.queren_call .buttons a{
    display:block;
    width: 3.6rem;
    height: 0.48rem;
    color:#FFFFFF;
    border-radius: 0.1rem;
    font-size: 0.24rem;
    text-align: center;
    margin-bottom: 0.3rem;
    line-height: 0.48rem;
    transform: translateX(30%);

}
.call_no .buttons a:nth-child(1),.queren_call .buttons a:nth-child(1){
    background:rgba(255,186,72,1);
    box-shadow:0px 5px 5px 0px rgba(255,106,34,0.4);
