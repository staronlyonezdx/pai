body{
    background:#fff;
}
.new_people{
    width:7.5rem;
    height:5.47rem;
    background: url("../../image/new_people/TOP@2x.png") no-repeat center center;
    background-size: 100% 100%;
    position: relative;
}
.new_people_rule{
    position: absolute;
    top:0.09rem;
    right:0;
    width:1.6rem;
    height:0.9rem;
}
.new_people_rule img{
    width:100%;
    height:100%;
}
.new_people_tit{
    width:3.1rem;
    height:0.44rem;
    position: absolute;
    bottom:0.2rem;
    margin-left: 0.36rem;
}
.new_people_tit img{
    width:100%;
    height:100%;
}
.new_people_main{
    background:#fff;
    margin-top: 0.2rem;
}
.new_people_con{
    width:6.9rem;
    /*height:1.8rem;*/
    margin:0 auto;
}
.new_people_img{
    width:1.8rem;
    height:1.8rem;
    position: relative;
}
.new_people_img .new_people_hink{
    width:100%;
    height:100%;
}
.new_people_img .new_people_label{
    width:0.9rem;
    height:0.36rem;
    background: url("../../image/new_people/juxing 9@2x.png") no-repeat center center;
    position: absolute;
    top:0;
    left: 0;
    color:#fff;
    font-size:0.12rem;
    text-align: center;
    line-height: 0.36rem;
}
.new_people_text{
    width:5.1rem;
}
.new_people_text p{
    margin-top: 0.1rem;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    color:#2b2b2b;
    font-size: 0.3rem;
    font-weight: 600;
    margin-left: 0.3rem;
}
.new_people_price{
    margin-left: 0.3rem;
    margin-top: 0.3rem;
}
.new_people_price span{
    color:#FD4747;
    font-size: 0.2rem;
    font-weight: 600;
}
.new_people_price span a{
    font-size: 0.24rem;
    color:#FD4747;
    font-weight: 600;
}
.new_people_price small{
    color:#787878;
    font-size: 0.22rem;
    margin-left: 0.2rem;
    text-decoration: line-through;
    font-weight: 400;
}
.new_people_num{
    margin-left: 0.3rem;
}
.new_people_num span{
    color:#787878;
    font-size: 0.22rem;
    margin-top: 0.4rem;
    display: inline-block;
}
.new_people_btn{
    width:1.6rem;
    height:0.64rem;
    background: #FF9000;
    box-shadow:0px 5px 5px 0px rgba(253,144,71,0.4);
    color:#fff;
    font-size:0.26rem;
    text-align:center;
    line-height: 0.64rem;
    border-radius: 0.04rem;
}
.new_people_header{
    width:0.6rem;
    height:0.6rem;
    border-radius: 50%;
    overflow: hidden;
}
.new_people_list{
    margin-top: 0.48rem;
    padding: 0 0.3rem;
    /*height:0.6rem;*/
}
.new_people_header img{
    width:100%;
    height:100%;
}
.new_people_list_view{
    width:6rem;
    margin-left: 0.3rem;
}
.new_people_rt p{
    width:1.2rem;
    font-size: 0.24rem;
    color:#2b2b2b;
    font-weight: 600;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
.new_people_rt span{
    font-size: 0.24rem;
    color:#2b2b2b;
    font-weight: 600;
    margin-left: 0.2rem;
}
.new_people_rt a{
    color:#2b2b2b;
    font-size: 0.24rem;
    font-weight: 600;
}
.new_people_val{
    margin-top: -0.05rem;
}
.new_people_val span{
    color:#aaaaaa;
    font-size: 0.22rem;
}
.new_people_val small{
    color:#aaaaaa;
    font-size: 0.22rem;
}
.new_people_call{
    margin-top: 0.63rem;
    padding-left:0.36rem;
}
.new_people_call_img{
    width:2.46rem;
    height:0.44rem;
    margin-bottom: 0.45rem;
}
.new_people_call_img img{
    width:100%;
    height:100%;
}
.new_people_call_list{
    padding-right:0.3rem;
    height:0.6rem;
    line-height: 0.6rem;
    margin-bottom: 0.2rem;
}
.new_call_header{
    width:0.6rem;
    height:0.6rem;
    border-radius: 50%;
    overflow: hidden;
}
.new_call_header img{
    width:100%;
    height:100%;
    display: block;
}
.new_people_call_list p{
    width:1.2rem;
    font-size: 0.24rem;
    color:#2b2b2b;
    font-weight: 600;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    margin-left: 0.3rem;
}
.new_people_call_list span{
    font-size: 0.24rem;
    color:#2b2b2b;
    font-weight: 600;
}
.new_people_bottom{
    width:7.5rem;
    height:5rem;
    overflow: hidden;
    margin-bottom: 0.6rem;
}
.new_people_bottom img{
    width:7.5rem;
    height:7.66rem;
}
.new_people_bottom_text{
    width:6.68rem;
    margin:0 auto;
    margin-top: 5.06rem;
}
.new_people_bottom_text p{
    text-align: center;
    color:#fff;
    font-size:0.24rem ;
}
.new_people_site{
    width:5.2rem;
    height:0.67rem;
    line-height: 0.67rem;
    text-align: center;
    background:#FF9C00;
    border-radius: 0.34rem;
    margin:0 auto;
    margin-top: 0.3rem;
    font-size:0.24rem;
    color:#ffffff;
}
.new_people_bottom_btn{
    width:7.5rem;
    height:0.98rem;
    line-height: 0.98rem;
    position: fixed;
    bottom:0;
    left: 0;
}
.new_people_bottom_btn div{
    height:100%;
    text-align: center;
    color:#ffffff;
    font-size:0.28rem ;
}
.new_people_bottom_btn .new_people_call_btn{
    width:4.7rem;
    background: #FFAE00;
}
.new_people_bottom_btn .new_people_debut_btn{
    width:2.8rem;
    background: #FF4122;
}
.new_people_pop{
    position: fixed;
    top:0;
    left: 0;
    right:0;
    bottom:0;
    background: rgba(0,0,0,0.20);
    z-index: 102;
    display: none;
}
.new_people_register{
    width:5.8rem;
    margin:0 auto;
    /*background:#fff;*/
    border-radius: 0.18rem;
    position: absolute;
    left:50%;
    margin-left: -2.8rem;
    z-index: 103;
    top:2.2rem;
}
.new_people_cancel{
    width:0.34rem;
    height:0.34rem;
    position: absolute;
    right:0.29rem;
    top:0.25rem;
}
.new_people_cancel img{
    width:100%;
    display: block;
    height:100%;
}
.new_people_tab{
    margin-bottom:0.26rem;
}
.new_people_tab_text{
    padding-left: 0.47rem;
}
.new_people_border{
    margin-right:0.75rem ;
}
.new_people_border span{
    color:#aaaaaa;
    font-size: 0.26rem;
    display: block;
    text-align: center;
}
.new_people_pitch span{
    color:#2b2b2b;
}
.new_people_pitch div{
    width:0.3rem;
    height:0.1rem;
    background:#FF9000;
    margin:0 auto;
    margin-top: 0.11rem;
}
.new_people_pop_main{
    display: none;
}
.new_people_pop_list{
    /*width:5.2rem;*/
    height:0.9rem;
    line-height: 0.9rem;
    background:#F5F5F5;
    padding-left: 0.15rem;
    padding-right: 0.15rem;
    margin-top: 0.2rem;
    border-radius: 0.09rem;
}
.new_people_pop_list img{
    width:0.62rem;
    height:0.62rem;
    margin-top: 0.15rem;

}
.new_people_pop_list input{
    width:4rem;
    margin-left: 0.11rem;
    background: #f5f5f5;
    margin-top: 0.26rem;
}
.new_people_pop_list .new_people_code{
    width:1.9rem;
}
.new_people_pop_list span{
    font-size: 0.26rem;
    color:#333333;
    /*margin-right: 0.2rem;*/
}
.new_people_register_btn{
    /*width:5.5rem;*/
    height:0.9rem;
    background:linear-gradient(90deg,#FF6A22,#FFB02D);
    border-radius:0.09rem;
    box-shadow:0 0.12rem 0.24rem 0 rgba(253,144,71,0.42);
    color:#fff;
    font-size:0.28rem;
    text-align: center;
    line-height: 0.9rem;
    margin-top: 0.34rem;
    margin-bottom: 0.3rem;
}
.new_people_line{
    /*display: none;*/
}
.new_people_yes_tit .new_people_yew_tit_img{
    width:3.97rem;
    height:0.44rem;
    margin:0 auto;
}
.new_people_yes_register{
    position: relative;
}
.new_people_friend{
    margin-top: 0.65rem;
}
.new_people_nameimg{
    width:0.6rem;
    height:0.6rem;
    overflow: hidden;
    border-radius: 50%;
}
.new_people_nameimg img{
    width:100%;
    height:100%;
}
.new_people_nametext{
    width:4.2rem;
    margin-left: 0.3rem;
}
.new_people_nametext p{
    color:#2b2b2b;
    font-size: 0.24rem;
}
.new_people_nametext span{
    color:#aaaaaa;
    font-size: 0.22rem;
}
.new_people_friend_btn{
    width:5.2rem;
    height:0.9rem;
    line-height: 0.9rem;
    background:linear-gradient(90deg,#FFA066,#FF5F34);
    border-radius: 0.09rem;
    text-align: center;
    color:#fff;
    font-size: 0.28rem;
    margin-top: 0.6rem;
    margin-bottom: 0.2rem;
}
.new_people_more_view{
    padding-right: 0.3rem;
}
.new_people_more_view p{
    font-size: 0.24rem;
    color:#FF9000;

}
.new_people_more{
    width:0.32rem;
    height:0.32rem;

}
.new_people_more img{
    width: 100%;
    height:100%;
    display:block;
}
.login-no{
    padding: 0.3rem;
    display: none;
    background:#fff;
}

.new_people_register .login-yes{
    /*display: none;*/
}

