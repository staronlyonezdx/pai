.call_yes_btns{
    /*margin-top:0.38rem ;*/
}
.call_yes_btns a {
    width: 2.24rem;
    height: 0.6rem;
}
.call_yes_btns img{
    width: 2.24rem;
    height: 0.6rem;
}
.call_yes_btns a:nth-child(1){
    margin-left: 0.63rem;
    margin-right: 0.35rem;
}
/* .call_yes imgs{

} */
.call_yes .imgs img:nth-child(1){
    width: 1.06rem;
    height: 1.06rem;
}
 .queren_call{
    display: none;
     /*background-color: white;*/
     /*padding-bottom: 0.2rem;*/
     width: 5.8rem;
     height: 8rem;
     background: url("../../image/new_people/pic_bg@2x.png") center center;
     background-size: 100%;
     position: relative;

}
.call_yes .call_bg,.queren_call .call_bg{
    display: flex;
    flex-flow: column nowrap;
    justify-content: space-around;
    width: 100%;
    height: 3.44rem;
    background: url("../../image/new_people/pic_beijing@2x.png") no-repeat;
}
.call_yes .call_bg .zan_yes,.queren_call .call_bg .zan_yes{
    display: block;
    font-size:0.3rem;
    font-weight:bold;
    color:#FFFFFF;
    line-height:0.6rem;
    text-align: center;
}

.call_yes .call_bg .click_cancle,.queren_call .call_bg .click_cancle{
    position: absolute;
    top: 0.2rem;
    right: 0;
    width: 0.56rem;
    height: 0.56rem;
}
.call_yes .call_bg .bg_middle,.queren_call .call_bg .bg_middle{
    display: flex;
}

.call_yes .call_bg .bg_middle .img_icon,.queren_call .call_bg .bg_middle .img_icon,.queren_call .call_bg .bg_middle .img_icon,.queren_call .call_bg .bg_middle .img_icon{
    width: 0.6rem;
    height: 0.6rem;
    margin-left: 0.5rem;
    border-radius: 50%;
}
.pers{
    margin-left: 0.3rem;
    width:70%;
    margin-top: -0.1rem;
}
.call_yes .call_bg .bg_middle .bg_top,.call_yes .call_bg .bg_middle .bg_bottom,.queren_call .call_bg .bg_middle .bg_top,.queren_call .call_bg .bg_middle .bg_bottom{
    color:#FFFFFF;
    font-size: 0.24rem;
    width:100%;
    overflow: hidden;
}
.new_name,.call_order{
    float: left;
}
.call_num,.new_t{
    float: right;
}
.call_yes .call_bg .bg_bottom,.queren_call .call_bg .bg_bottom{
    position: relative;
}
.call_yes .call_bg .bg_bottom img,.queren_call .call_bg .bg_bottom img{
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    width: 3.62rem;
    height: 0.9rem;
}
.fight{
    font-size: 0.24rem;
    color: #FFFFFF;
    width: 1.5rem;
    height: 0.23rem;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    padding-bottom: 0.24rem;
    line-height: 0.23rem;
}
.fight:visited{
    color:#FFF;
}
.red_packet{
    width:100%;
    color:#AAAAAA;
    font-size:0.26rem ;
    padding: 0.3rem 0;
    text-align: center;
}
.download{
    width: 100%;
    display: flex;
    justify-content: space-around;
}
.download  span{
    display: inline-block;
    width: 2.2rem;
    height: 0.48rem;
    border:0.02rem #FFAA2C solid;
    text-align: center;
    line-height: 0.48rem;
    font-size: 0.24rem;
    color:#FFAA2C;
    border-radius: 0.1rem;
}
.imgs{
    display: flex;
    flex-flow: column;
    align-items: center;
    margin-top: 0.4rem;
    margin-bottom: 0.2rem;
    margin-left: 0.5rem;
}
