.syuclub_view{
    width:7.5rem;
    height:12.06rem;
    background:url("../../image/syuclub/icon_img@2x.png") no-repeat center center;
    background-size:100% 100%; 
    overflow: hidden;
}
.syuclub_phone,.syuclub_pass{
    width:4.48rem;
    height:0.92rem;
    background: #F7E9C6;
    margin:0 auto;
}
.syuclub_phone{
    margin-top:5.01rem;
}
.syuclub_pass{
    margin-top:0.4rem;
}
.syuclub_phone input,.syuclub_pass input{
    height:100%;
    background: #F7E9C6;
    text-indent: 0.36rem;
    color:#997430;
    font-size: 0.3rem;
    font-family:PingFangSC-Regular;
}
.syuclub_phone input{
    width:100%;
}
.syuclub_pass input{
    width:3.48rem;
}
.syuclub_pass .syuclub_open{
    width:1rem;
    height:100%;
}
.syuclub_pass .syuclub_open img{
    /* display:block; */
    width:0.36rem;
    height:0.22rem;
    margin:0 auto;
    margin-top: 0.36rem;
    display: none;
}
.syuclub_pass .syuclub_open .show{
    display: block;
}
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
    color: #CDAD6D;
    font-size: 0.3rem;
    font-family:PingFangSC-Regular;
}

input:-moz-placeholder, textarea:-moz-placeholder {
    color: #CDAD6D;
    font-size: 0.3rem;
    font-family:PingFangSC-Regular;
}

input::-moz-placeholder, textarea::-moz-placeholder {
    color: #CDAD6D;
    font-size: 0.3rem;
    font-family:PingFangSC-Regular;
}

input:-ms-input-placeholder, textarea:-ms-input-placeholder {
    color: #CDAD6D;
    font-size: 0.3rem;
    font-family:PingFangSC-Regular;
}
.syuclub_text{
    font-family: "SourceHanSerifCN-Heavy";
    font-size: 0.26rem;
    color:#ECD08F;
    text-align: center;
    margin-top: 0.74rem;
    font-weight: 600;
}
.syuclub_btn{
    width:5.25rem;
    height:1.3rem;
    margin:0 auto;
    margin-top:0.16rem;
}
.syuclub_btn img{
    width:100%;
    height:100%;
}
.layui-layer-btn .layui-layer-btn0{
    border-right: 0!important;
    color: #000000!important;
    font-size: 0.3rem!important;
}
