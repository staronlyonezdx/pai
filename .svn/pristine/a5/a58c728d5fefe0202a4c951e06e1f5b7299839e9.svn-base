    position: relative;
    height: 1.8rem;
}
.index-nav div img {
    width: 1.4rem;
    height: 1.4rem;
    display: block;
    margin: 0 auto;
}
.index-nav div p {
    font-size: 0.26rem;
    color: #333333;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    position: absolute;
    bottom: 0.1rem;
    width: 100%;
}
.index-cont {
    background: #fff;
    border-radius: 0.2rem;
    width: 7.1rem;
    margin: 0 auto;
    border-top-right-radius: 0.2rem;
    border-top-left-radius: 0.2rem;
