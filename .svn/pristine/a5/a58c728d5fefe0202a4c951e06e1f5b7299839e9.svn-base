}
.index-nav::after, .index-notice::after, .index-notice-title::after, .index-buy-tit::after, .index-buy-gd ul li::after, .index-act::after, .index-pro-item-num {
    clear: both;
    display: table;
    content: '';
}
.index-nav div {
    width: 20%;
