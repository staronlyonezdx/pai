/* 红包 */
.red {
	width: 1.2rem;
	height: 1.53rem;
	position: fixed;
	bottom: 2rem;
	right: 0;
	z-index: 100;
	display: none;
}

.dd {
    animation: shake 4s infinite;
    -o-animation: shake 4s infinite;
    -webkit-animation: shake 4s infinite;
    -moz-animation: shake 4s infinite;
}
@keyframes shake {
    0%, 25% {
        -webkit-transform: translateX(0);
    }
    2.5%,
    7.5%,
    12.5%,
    17.5%,
    22.5% {
        -webkit-transform: translateX(-5px);
    }
    5%,
    10%,
    15%,
    20% {
        -webkit-transform: translateX(5px);
	}
	100% {
        -webkit-transform: translateX(0);
    }
}
@-o-keyframes shake {
    /* Opera */
    0%, 25% {
        -webkit-transform: translateX(0);
    }
    2.5%,
    7.5%,
    12.5%,
    17.5%,
    22.5% {
        -webkit-transform: translateX(-5px);
    }
    5%,
    10%,
    15%,
    20% {
        -webkit-transform: translateX(5px);
	}
	100% {
        -webkit-transform: translateX(0);
    }
}
@-webkit-keyframes shake {
    /* Safari 和 Chrome */
    0%, 25% {
        -webkit-transform: translateX(0);
    }
    2.5%,
    7.5%,
    12.5%,
    17.5%,
    22.5% {
        -webkit-transform: translateX(-5px);
    }
    5%,
    10%,
    15%,
    20% {
        -webkit-transform: translateX(5px);
	}
	100% {
        -webkit-transform: translateX(0);
    }
}
@-moz-keyframes shake {
    /* Firefox */
    0%, 25% {
        -moz-transform: translateX(0);
    }
    2.5%,
    7.5%,
    12.5%,
    17.5%,
    22.5% {
        -moz-transform: translateX(-5px);
    }
    5%,
    10%,
    15%,
    20% {
        -moz-transform: translateX(5px);
	}
	100% {
        -webkit-transform: translateX(0);
    }
}
