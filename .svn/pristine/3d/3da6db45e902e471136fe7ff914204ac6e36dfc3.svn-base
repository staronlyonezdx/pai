.search_top {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 4;
    background-color: white;

}
.search_top_hearder {
    width: 100%;
    height: 0.88rem;
    background: linear-gradient(270deg, rgba(241, 57, 65, 1) 0%, rgba(252, 68, 107, 1) 100%);
    position: fixed;
    z-index: 3;
    top: 0;
    left: 0;
}

.goback {
    width: 0.22rem;
    height: 0.36rem;
    margin-top: 0.24rem;
    margin-left: 0.48rem;
    margin-right: 0.36rem;
}

.goback img {
    width: 100%;
    height: 100%;
}

.index_search {
    width: 5.4rem;
    height: 0.6rem;
    background: rgba(255, 255, 255, 1);
    opacity: 1;
    border-radius: 0.3rem;
    margin-top: 0.14rem;
}

.index_search img {
    width: 0.24rem;
    height: 0.24rem;
    margin-left: 1.28rem;
    margin-top: 0.16rem;
    margin-right: 0.16rem;
}

.index_search input {
    height: auto;
    margin: 0;
    padding: 0;
    width: 4.4rem;
    /*margin-left: 0.12rem;*/
    margin-top: 0.15rem;
    border: none;
    /*line-height: 0.6rem;*/
    background: white;
    color: #333333;
    /*caret-color: red;*/
    font-size: 0.24rem;
}

.search_btn {
    font-size: 0.28rem;
    font-family: PingFang SC;
    font-weight: 500;
    line-height: 0.88rem;
    color: rgba(255, 255, 255, 1);
    opacity: 1;
    margin-left: 0.2rem;
}

.search_top_content{
    padding-left: 0.4rem;
    margin-top: 0.88rem;
    display: none;
}
.search_top_item {

}

.search_top_title {
    font-size:0.28rem;
    font-family:PingFang SC;
    font-weight:600;
    line-height:0.8rem;
    color:rgba(51,51,51,1);
    opacity:1;
}
.search_item{

}
.search_item div{
    padding: 0rem 0.23rem;
    background: #f5f5f5;
    border-radius: 0.5rem;
    margin-right: 0.16rem;
    margin-bottom: 0.32rem;
    font-size: 0.24rem;
    color: #666666;
    height: 0.48rem;
    line-height: 0.48rem;
}
.del_all{
    width: 0.3rem;
    height: 0.3rem;
    margin-top: 1rem;
    margin-right: 0.4rem;
    display: none;
}
.del_all img{
    width: 100%;
    height: 100%;
}
.layui-layer-btn1{
    color:#F13944!important;
}
.mescroll{
    margin-top: 0.88rem;
}
.content_item {
    width: 3.34rem;
    background-color: white;
    margin-top: 0.2rem;
    margin-left: 0.3rem;
    /*margin-right: 0.2rem;*/
    padding-bottom: 0.2rem;
    border-radius: 0.16rem;
}

/*.content_item:nth-child(2n){*/
/*margin-left:0 ;*/
/*margin-right:0 ;*/
/*}*/
.info_img {
    width: 3.34rem;
    height: 3.34rem;
    /*background: red;*/
    border-radius: 0.16rem 0.16rem 0 0;
}

.content_item_info {
    padding-left: 0.24rem;
    margin-top: 0.24rem;
}

.content_info_name {
    width: 2.9rem;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 0.26rem;
    font-family: PingFang SC;
    font-weight: 500;
    color: rgba(0, 0, 0, 1);
    opacity: 1;
}

.progress {
    margin-top: 0.06rem;
}

.progress_main {
    width: 2.2rem;
    height: 0.16rem;
    background: rgba(255, 230, 230, 1);
    opacity: 1;
    border-radius: 0.08rem;
    margin-top: 0.1rem;
}

.progress_main span {
    height: 100%;
    display: block;
    border-radius: 0.08rem;
    background: rgba(255, 43, 43, 1);
}

.progress_num {
    font-size: 0.24rem;
    font-family: PingFang SC;
    font-weight: 500;
    line-height: 0.34rem;
    color: rgba(0, 0, 0, 1);
    opacity: 1;
    margin-left: 0.1rem;
}

.content_info_data {
    margin-top: 0.16rem;
}

.info_price {
    font-size: 0.28rem;
    font-family: PingFang SC;
    font-weight: 600;
    line-height: 0.4rem;
    color: rgba(255, 43, 43, 1);
    opacity: 1;
}

.info_price small {
    font-size: 0.2rem;
}

.join {
    font-size: 0.24rem;
    font-family: PingFang SC;
    font-weight: 400;
    line-height: 0.34rem;
    color: rgba(170, 170, 170, 1);
    opacity: 1;
    margin-right: 0.1rem;
}
.empty-btn{
    width:1.6rem;
    height:0.68rem !important;
    background:rgba(241,57,67,1) !important;
    opacity:1;
    border-radius:0.12rem !important;
    padding: 0 !important;
    border: none !important;
    text-align: center;
    line-height: 0.68rem;
    font-size:0.26rem;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(255,255,255,1) !important;
}
.peanut_item_data{
    font-size:0.3rem;
    font-family:PingFang SC;
    font-weight:600;
    line-height:0.42rem;
    color:rgba(255,198,10,1);
    margin-top: 0.2rem;
}
.peanut_item_data>img{
    width: 0.36rem;
    height: 0.38rem;
}
.content_info_name>img{
    width: 0.84rem;
    height: 0.36rem;
}
.is_peanut{
    background: #FDD42F !important;
    color: black !important;
}
.is_peanut_sheng{
    width: 100%;
    height: 0.6rem;
    display: inline-block;
    background:rgba(0,0,0,0.54);
    text-align: center;
    line-height: 0.6rem;
    font-size:0.24rem;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(255,255,255,1);
    position: relative;
    bottom: 0;
    left: 0;
}
