.header_nav{
    background: #FDD42F;
}
.peanut_company{
    padding:0.3rem 0.3rem 0 0.3rem;
}
.peanut_company_con{
    width:6.9rem;
    background: #fff;
    border-radius: 0.18rem;
    overflow: hidden;
    margin-bottom: 0.2rem;
}
.peanut_company_top{
    width:100%;
    height:0.88rem;
    border-bottom:1px solid #fafafa;
}
.peanut_company_top small{
    color:#000000;
    font-size: 0.26rem;
    font-family:PingFang SC;
    font-weight:600;
    line-height: 0.88rem;
    margin-right:0.3rem;
}
.peanut_company_tit{
    color:#000000;
    font-size:0.24rem;
    font-family:PingFang SC;
    font-weight:600;
    line-height: 0.88rem;
    margin-left: 0.3rem;
}
.peanut_company_btn{
    width:1.68rem;
    height:0.6rem;
    background: #FDD42F;
    border-radius: 0.08rem;
    margin-top: 0.14rem;
    margin-right:0.2rem;
}
.peanut_company_btn span{
    color: #000000;
    font-size:0.26rem;
    font-family:PingFang SC;
    font-weight:600;
    line-height: 0.6rem;
    vertical-align: middle;
    margin-left: 0.18rem;
}
.peanut_company_btn img{
    width:0.11rem;
    height:0.19rem;
}
.peanut_company_main{
    padding: 0.2rem 0.3rem;
}
.peanut_company_img{
    width:1.6rem;
    height:1.6rem;
    border-radius: 0.12rem;
}
.peanut_company_img img{
    width:100%;
    height:100%;
}
.peanut_company_text{
    margin-left: 0.2rem;
}
.peanut_company_text_top{
    margin-top: 0.08rem;
}
.peanut_company_text_top p{
    width:3.6rem;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    float: left;
    font-weight:600;
}
.peanut_company_text_top span{
    float:right;
    margin-left: 0.5rem;
    margin-top: 0.25rem;
    font-weight:600;
}
.peanut_company_text_bottom{
    margin-top: 0.12rem;
}
.peanut_company_text_bottom .peanut_company_pic{
    width:0.46rem;
    height:0.46rem;
    border-radius: 50%;
}
.peanut_company_text_bottom .peanut_company_huasheng{
    width:0.36rem;
    height:0.38rem;
}
.peanut_company_text_bottom img{
    vertical-align: middle;
}
.peanut_company_text_bottom p{
    display: inline;
    vertical-align: middle;
    color: #000000;
    font-size:0.24rem;
    font-family:PingFang SC;
    font-weight:600;
    padding:0 0.08rem;
}
.peanut_company_text_bottom span{
    vertical-align: middle;
    color:#FFC60A;
    font-size:0.26rem;
    font-family:PingFang SC;
    font-weight:600;
    padding:0 0 0 0.08rem;
}
